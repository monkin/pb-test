{
  "name": "pb-test",
  "version": "1.0.0",
  "scripts": {
    "tscss": "tscss --path client/**/*.pcss",
    "start": "concurrently --kill-others \"tscss --path client/**/*.pcss -w\" \"webpack-dev-server --mode development --inline --dev\" \"ts-node server/serverful.ts\"",
    "deploy-client": "webpack --mode production && aws s3 cp ./build/client/ s3://monkin-pb-test/ --recursive --acl public-read",
    "deploy-server": "tsc -p tsconfig.server.json && cp serverless.package.json build/server/package.json && npm install build/server --prefix build/server --production && bestzip build/lambda.zip build/server/* && aws lambda update-function-code --function-name monkin-pb-test --zip-file fileb://build/lambda.zip",
    "deploy": "concurrently \"npm run deploy-client\" \"npm run deploy-server\""
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "express": "4.16.3",
    "http-errors": "1.7.0",
    "multer": "1.3.1",
    "pb-test-serverless": "file:build/server",
    "reflect-metadata": "0.1.12",
    "routing-controllers": "0.7.7",
    "serverless-http": "1.6.0"
  },
  "devDependencies": {
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "reselect": "3.0.1",
    "classnames": "2.2.6",
    "axios": "0.15.3",
    "redux-devtools-extension": "2.13.5",
    "@types/react": "16.4.8",
    "@types/react-dom": "16.0.7",
    "@types/redux": "3.6.0",
    "@types/react-redux": "6.0.6",
    "@types/reselect": "2.2.0",
    "@types/classnames": "2.2.6",
    "@types/redux-devtools-extension": "2.13.2",
    "tscss": "0.2.1",
    "source-map-loader": "0.2.3",
    "ts-loader": "4.4.2",
    "typescript": "3.0.1",
    "cssnano": "4.0.5",
    "style-loader": "0.22.1",
    "css-loader": "1.0.0",
    "postcss-less": "2.0.0",
    "postcss-import": "12.0.0",
    "postcss-loader": "2.1.6",
    "autoprefixer": "9.1.0",
    "webpack": "4.16.5",
    "file-loader": "1.1.11",
    "concurrently": "3.6.1",
    "webpack-bundle-tracker": "0.3.0",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5",
    "ts-node": "7.0.0",
    "bestzip": "1.1.4"
  }
}
